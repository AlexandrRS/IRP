<?xml version="1.0" encoding="UTF-8"?>
<build-type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" uuid="9a3d9fcb-39f0-45be-828d-fab1e9b20e9e" xsi:noNamespaceSchemaLocation="https://www.jetbrains.com/teamcity/schemas/2020.1/project-config.xsd">
  <name>SonarQube</name>
  <description />
  <settings>
    <parameters>
      <param name="env.Database1CName" value="%dep.IRP_GitHubUpdate.env.Database1CName%" />
    </parameters>
    <build-runners>
      <runner id="RUNNER_1" name="SonarQube Branch or PR Prepare" type="SonarQubeBranch">
        <parameters>
          <param name="teamcity.step.mode" value="default" />
        </parameters>
      </runner>
      <runner id="RUNNER_2" name="Sonar Start scan" type="sonar-plugin">
        <parameters>
          <param name="additionalParameters" value="%dep.IRP_GitHubUpdate.env.SonarParamCover%" />
          <param name="sonarProjectKey" value="%teamcity.project.id%" />
          <param name="sonarProjectName" value="%system.teamcity.projectName%" />
          <param name="sonarProjectSources" value="src" />
          <param name="sonarProjectVersion" value="%build.number%" />
          <param name="sonarServer" value="4ebba3c9-2abc-4f9a-9c9c-7760b6ad6607" />
          <param name="teamcity.step.mode" value="default" />
          <param name="teamcity.tool.sonarquberunner" value="%teamcity.tool.sonar-qube-scanner.DEFAULT%" />
        </parameters>
      </runner>
      <runner id="RUNNER_3" name="" type="DropDB">
        <parameters>
          <param name="teamcity.step.mode" value="default" />
        </parameters>
      </runner>
    </build-runners>
    <vcs-settings>
      <vcs-entry-ref root-id="IRP_GitTC" />
    </vcs-settings>
    <requirements />
    <build-triggers />
    <build-extensions>
      <extension id="BUILD_EXT_1" type="commit-status-publisher">
        <parameters>
          <param name="github_authentication_type" value="token" />
          <param name="github_host" value="https://api.github.com" />
          <param name="github_oauth_user" value="DitriXNew" />
          <param name="publisherId" value="githubStatusPublisher" />
          <param name="secure:github_access_token" value="credentialsJSON:7f350f35-7665-4b1b-b6e1-d624efb86189" />
        </parameters>
      </extension>
      <extension id="BUILD_EXT_2" type="pullRequests">
        <parameters>
          <param name="authenticationType" value="token" />
          <param name="filterAuthorRole" value="MEMBER" />
          <param name="providerType" value="github" />
          <param name="secure:accessToken" value="credentialsJSON:7f350f35-7665-4b1b-b6e1-d624efb86189" />
          <param name="vcsRootId" value="IRP_GitTC" />
        </parameters>
      </extension>
    </build-extensions>
    <dependencies>
      <depend-on sourceBuildTypeId="IRP_RunVA">
        <options>
          <option name="run-build-if-dependency-failed" value="RUN_ADD_PROBLEM" />
          <option name="take-started-build-with-same-revisions" value="true" />
          <option name="take-successful-builds-only" value="true" />
        </options>
      </depend-on>
    </dependencies>
    <cleanup />
  </settings>
</build-type>

